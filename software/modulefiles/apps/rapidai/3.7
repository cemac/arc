#%Module 1.0
package require flavours
flavours init

proc ModulesHelp { } {
        global version
        puts stderr "   This module loads anaconda pyth 3.7 "
        Version $version
}

module-whatis "Anaconda for WRFCHEM"
flavours conflict python
flavours root $::env(CEMAC_DIR)/software/apps/rapidai/3.7
flavours revision 1
flavours commit

setenv CONDA_PREFIX /nobackup/CEMAC_rapid_ai/anaconda3
setenv CONDA_PATH /nobackup/CEMAC_rapid_ai/anaconda3/etc/profile.d
setenv CONDA_PYTHON_EXE /nobackup/CEMAC_rapid_ai/anaconda3/bin/python
setenv ANACONDA_PATH /nobackup/CEMAC_rapid_ai/anaconda3


flavours prepend-path PATH bin condabin
flavours cleanup
